{
  "name": "n8n-nodes-skillsbank",
  "version": "1.0.0",
  "description": "n8n nodes for Skills Bank - Execute and manage reusable code skills",
  "keywords": [
    "n8n-community-node-package",
    "skills",
    "automation",
    "code-execution"
  ],
  "license": "MIT",
  "homepage": "https://github.com/MauricioPerera/n8n-nodes-skillsbank",
  "author": {
    "name": "Mauricio Perera",
    "email": "mauricio.perera@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MauricioPerera/n8n-nodes-skillsbank.git"
  },
  "main": "dist/nodes/SkillsBank/SkillsBank.node.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SkillsBankApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SkillsBank/SkillsBank.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
